{
  "name": "opcua-client",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "babel": "babel src --out-dir dist --extensions \".ts,.tsx\"",
    "build": "tsc -p .",
    "build:live": "ts-node -r tsconfig-paths/register -r tsm src/index.ts",
    "build:production": "npm run env:production && npm run babel",
    "env:development": "cp -r ./src/environment/environment.development.ts ./src/environment/environment.ts",
    "env:local": "cp -r ./src/environment/environment.local.ts ./src/environment/environment.ts",
    "env:production": "cp -r ./src/environment/environment.production.ts ./src/environment/environment.ts",
    "lint": "eslint --ext .js,.ts .",
    "lint-fix": "eslint --fix --ext .js,.ts .",
    "lint-prettier": "prettier '**/*.{yml,yaml,md}' --write",
    "lint-prettier-ci": "prettier '**/*.{yml,yaml,md}' --check",
    "postbuild": "cp package.json dist/package.json",
    "start": "npm run env:local && npm run build:live",
    "start:development": "npm run env:development && npm run build:live",
    "start:production": "npm run env:production && npm run build:production",
    "test": "jest",
    "tsc": "tsc"
  },
  "author": "Johnathan Sanders",
  "license": "UNLICENSED",
  "description": "",
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@types/jest": "^29.5.14",
    "@types/jsonpath": "^0.2.4",
    "@types/node": "^22.10.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.16.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "tsm": "^2.3.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "ethernet-ip": "^1.2.5",
    "jsonpath": "^1.1.1",
    "ltx": "^3.1.1",
    "mqtt": "^5.10.3",
    "node-opcua": "^2.138.1",
    "rxjs": "^7.8.1"
  }
}
